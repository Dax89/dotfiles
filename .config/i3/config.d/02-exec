# File Manager
$exec filemanagerd

# Device Manager
$exec udiskie --tray

# Bluetooth Manager
$exec blueman-applet

# Clipboard Tool
$exec greenclip daemon > /dev/null

# Wallpaper
$exec nitrogen --restore

# OpenRGB
$exec ~/.me/scripts/openrgb.sh

# Start XDG autostart .desktop files using dex. See also
# https://wiki.archlinux.org/index.php/XDG_Autostart
$exec dex --autostart --environment i3

# Polybar
$exec ~/.config/polybar/polybar.sh

# vi: ft=i3config
